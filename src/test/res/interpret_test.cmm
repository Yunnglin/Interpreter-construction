
// interpret test

int Fibonacci(int n) {
    if (n<=0) {
        return -1;
    }
    if (n == 1) {
        return 1;
    }
    if (n == 2) {
        return 1;
    }

    return Fibonacci(n-1) + Fibonacci(n-2);
}


int main()
{
    int a = 1;
    while (a > 0) {
        read a;
        write Fibonacci(a);
    }
}
